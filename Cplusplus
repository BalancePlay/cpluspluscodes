C++

###Swap Array

#include<iostream>

using namespace std;

void swap(int *x,int *y)
{
    *x = *y + -*x +(*y = *x);
    int m = *x;
    int n = *y;
    cout<<"Value of a and b after swap"<<endl;
    cout<<m<<"and"<<n;
}
int main()
{
    int a,b,temp;
    cout<<"Enter two values";
    cin>>a>>b;
    cout<<"Value of a and b before swap:"<<endl;
    cout<<a<<"and"<<b<<endl;
    swap(&a,&b);
    return 0;
}

>>Distinct Elements array

#include<iostream>
using namespace std;
int main()
{
    int A[10]={11,9,8,5,3,2,90,88,2,22};
    int uq=1;
    for(int i=1;i<10;i++)
    {   
        int j;
        if(i == j)
            uq += 1;
        for( j=0;j<i;j++)
            if(A[i] == A[j])
                break;
            
        if(i==j)
            uq +=1;
        
    }
    cout<<"The unique elements are: "<<uq;
    
    return 0;
}
###Union Array ( little tuning req to remove garbage value )

#include<iostream>
using namespace std;
int main()
{
    int A[10]={11,9,8,5,3,2,90,88,2,22};
    int uq1=1;
    for(int i=1;i<10;i++)
    {   
        int j;
        for( j=0;j<i;j++)
            if(A[i] == A[j])
                break;
            
        if(i==j)
            uq1 +=1;
        
    }
    cout<<"The unique elements are: "<<uq1<<endl;
    
    int B[10]={1,8,8,44,33,17,19,8,2,22};
    int uq2=1;
    for(int i=1;i<10;i++)
    {   
        int j;
        for( j=0;j<i;j++)
            if(B[i] == B[j])
                break;
            
        if(i == j)
            uq2 +=1;
        
    }
    cout<<"The unique elements are: "<<uq2<<endl;
    
    int Uarr[uq1+uq2];
    int flag=0;int m=1;
    Uarr[0]=A[0];
    if(A[0]!=B[0])
    {
        Uarr[m]=B[0];
        m+=1;
    }
    
    for(int i=1;i<10;i++)
    {   int j;
        for(j=0;j<i;j++)
        {
            if(A[i]==A[j])
                break;
        }
        
        if(i==j)
        {
            Uarr[m]=A[i];
            m+=1;
        }
    }
    cout<<m<<endl;
    for(int i=1;i<10;i++)
    {
        int j;
        for(j=0;j<m;j++)
        {
            if(B[i]== Uarr[j])
                break;
        }
        
        if(j==m)
        {
            Uarr[m]=B[i];
            m+=1;
        }
    }
    cout<<"the union of Array A and Array B is : ";
    for(int i=0;i<sizeof(Uarr)/sizeof(Uarr[0]);i++)
    {
        cout<<Uarr[i]<<" ";
    }
    
    return 0;
}

###Unique Elements

#include<iostream>
using namespace std;
int main()
{
    int A[10]={11,9,8,5,3,2,90,88,2,22};
    int uq1=1;
    for(int i=1;i<10;i++)
    {   
        int j;
        for( j=0;j<i;j++)
            if(A[i] == A[j])
                break;
            
        if(i==j)
            uq1 +=1;
        
    }
    cout<<"The unique elements are: "<<uq1<<endl;
    
    int B[10]={1,8,8,44,33,17,19,8,2,22};
    int uq2=1;
    for(int i=1;i<10;i++)
    {   
        int j;
        for( j=0;j<i;j++)
            if(B[i] == B[j])
                break;
            
        if(i == j)
            uq2 +=1;
        
    }
    cout<<"The unique elements are: "<<uq2<<endl;
    
    int Uarr[uq1+uq2];
    int flag=0;int m=1;
    Uarr[0]=A[0];
    if(A[0]!=B[0])
    {
        Uarr[m]=B[0];
        m+=1;
    }
    
    for(int i=1;i<10;i++)
    {   int j;
        for(j=0;j<i;j++)
        {
            if(A[i]==A[j])
                break;
        }
        
        if(i==j)
        {
            Uarr[m]=A[i];
            m+=1;
        }
    }
    cout<<m<<endl;
    for(int i=1;i<10;i++)
    {
        int j;
        for(j=0;j<m;j++)
        {
            if(B[i]== Uarr[j])
                break;
        }
        
        if(j==m)
        {
            Uarr[m]=B[i];
            m+=1;
        }
    }
    cout<<"the union of Array A and Array B is : ";
    for(int i=0;i<sizeof(Uarr)/sizeof(Uarr[0]);i++)
    {
        cout<<Uarr[i]<<" ";
    }
    
    return 0;
}

#Multiples of 5 at even places

#include <iostream>
using namespace std;
int main()
{   
    int size;
    cout<<"Enter size of array";
    cin>>size;
    int arr[size];
    cout<<"Enter the elements of array"<<endl;
    for(int i=0;i<size;i++)
    {
        cin>>arr[i];
    }
    cout<<"The multiples of 5 at even places are:";
    int n=sizeof(arr)/sizeof(arr[0]);
    for(int i=0,j=2;i<n/2,j<n;i++,j+=2)
    {  
         if(arr[j]%5 == 0)
            cout<<arr[j]<<"["<<j<<"]"<<" ";
    }    
    
    return 0;
}

#Repeating elements

#include<iostream>
using namespace std;
int main()
{
    int n;
    cout<<"Enter the size of array";
    cin>>n;
    int Arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>Arr[i];
    }
    cout<<"Array: ";
    for(int i=0;i<n;i++)
    {
        cout<<Arr[i];
    }
    for(int j=0;j<n;j++)
    {
       
        for(int k=j+1;k<n;k++)
        {
            if(Arr[j]==Arr[k])
                cout<<endl<<"The repeating element is "<<Arr[j]<<endl;
            else
                continue;
        }
    }
}

#Product Of Subarrays

#include<iostream>
using namespace std;
int main()
{
    int Arr[]={10,3,7};
    int size = sizeof(Arr)/sizeof(Arr[0]);
    int prod=1;
    for (int i=0;i<size;i++)
    {
        for(int j=i;j<size;j++)
        {
            prod *= Arr[j];
        }
    }
    cout<<"The product of subarrays is"<<prod;
}


